	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "head",
			"skipFiles": ["<node_internals>/**"],
			"program": "${workspaceFolder}/head/bin/www"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "backend-v1",
			"skipFiles": ["<node_internals>/**"],
			"env": { "VERSION": "v1" },
			"program": "${workspaceFolder}/backend/bin/www"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "backend-v2",
			"skipFiles": ["<node_internals>/**"],
			"env": {
				"VERSION": "v2"
			},
			"program": "${workspaceFolder}/backend/bin/www"
		}
	]
}
